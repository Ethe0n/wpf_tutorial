<Application x:Class="TutorialApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TutorialApp"
             xmlns:view="clr-namespace:TutorialApp.View"
             xmlns:viewmodel="clr-namespace:TutorialApp.ViewModel"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewmodel:LibraryViewModel}">
            <view:LibraryControl/>
        </DataTemplate>
    </Application.Resources>
</Application>
